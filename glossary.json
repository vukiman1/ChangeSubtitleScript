{
  "rules": [
    {
      "pattern": "(?i)(?<![A-Za-zÀ-ỹ])hình\\s*ảnh(?![A-Za-zÀ-ỹ])|(?<![A-Za-zÀ-ỹ])hinh\\s*anh(?![A-Za-zÀ-ỹ])",
      "replace": "Image",
      "enabled": false,
      "notes": "hình ảnh -> Image"
    },
    {
      "pattern": "(?i)(?<![A-Za-zÀ-ỹ])nút(?![A-Za-zÀ-ỹ])|(?<![A-Za-zÀ-ỹ])nut(?![A-Za-zÀ-ỹ])",
      "replace": "node",
      "enabled": true,
      "notes": "nút -> node"
    },
    {
      "pattern": "(?i)(?<![A-Za-zÀ-ỹ])thùng\\s*chứa(?![A-Za-zÀ-ỹ])|(?<![A-Za-zÀ-ỹ])thùng\\s*chưa(?![A-Za-zÀ-ỹ])|(?<![A-Za-zÀ-ỹ])thung\\s*chua(?![A-Za-zÀ-ỹ])",
      "replace": "container",
      "enabled": true,
      "notes": "thùng chứa -> container"
    },
    {
      "pattern": "(?i)(?<![A-Za-zÀ-ỹ])vùng\\s*chứa(?![A-Za-zÀ-ỹ])|(?<![A-Za-zÀ-ỹ])vung\\s*chua(?![A-Za-zÀ-ỹ])",
      "replace": "Container",
      "enabled": true,
      "notes": "vùng chứa -> Container"
    },
    {
      "pattern": "(?i)(?<![A-Za-zÀ-ỹ])chế\\s*độ\\s*tách\\s*rời(?![A-Za-zÀ-ỹ])|(?<![A-Za-zÀ-ỹ])che\\s*do\\s*tach\\s*roi(?![A-Za-zÀ-ỹ])",
      "replace": "Detach Mode",
      "enabled": true,
      "notes": "chế độ tách rời -> Detach Mode"
    }
  ]
}
